{
  "name": "@gisatcz/ptr-utils",
  "version": "1.3.4",
  "description": "Panther FE utils",
  "keywords": [
    "panther",
    "gisat"
  ],
  "homepage": "https://github.com/gisat/ptr-utils",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "lib"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gisat/ptr-utils"
  },
  "dependencies": {
    "@gisatcz/ptr-core": "^1.3.8",
    "@turf/turf": "5.1.6",
    "chroma-js": "2.1.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.19",
    "moment": "2.24.0",
    "re-reselect": "3.4.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "auto": "^9.43.1",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-externals": "^2.1.3",
    "set-tz": "0.2.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11"
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "tests"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "run-s clean && run-p start:es start:cjs start:lib:es",
    "start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
    "start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
    "start:es": "cross-env NODE_ENV=es rollup -c -w",
    "build": "run-s clean && run-p build:es build:cjs build:lib:es",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
    "test": "cross-env NODE_ENV=cjs rollup -c ./rollup.test.config.js && cross-env TZ=utc mocha -- --require source-map-support/register",
    "test:watch": "npm-watch test"
  }
}
